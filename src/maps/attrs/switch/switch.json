{
	"w-switch/value": {
		"description": "绑定值",
		"optionType": "Boolean",
		"defaultValue": "-",
		"version": "0.4.0",
		"require": "false"
	},
	"w-switch/disabled": {
		"description": "设置禁用状态",
		"optionType": "Boolean",
		"defaultValue": "-",
		"version": "0.4.0",
		"require": "false"
	},
	"w-switch/loading": {
		"description": "设置载入状态",
		"optionType": "Boolean",
		"defaultValue": "-",
		"version": "0.4.0",
		"require": "false"
	},
	"w-switch/stop": {
		"description": "点击事件是否冒泡",
		"optionType": "Boolean",
		"defaultValue": "-",
		"version": "0.4.0",
		"require": "false"
	},
	"w-switch/size": {
		"description": "设置大小",
    "options": [
      "small",
      "large",
      "不设置"
    ],
