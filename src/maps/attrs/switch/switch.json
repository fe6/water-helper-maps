		"require": "false"
	},
	"w-switch/change": {
		"description": "改变触发，返回 {Event对象,当前状态}({ev,status})",
		"optionType": "Function",
		"defaultValue": "({ev,status}) => {}",
		"version": "0.4.0",
		"require": "false"
	}
