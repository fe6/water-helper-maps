		"defaultValue": "-",
		"version": "0.20.0",
		"require": "false"
	},
	"w-datepicker/change": {
		"description": "改变的方法，返回 {绑定值, 输入框显示值}({value, text})",
		"optionType": "Function",
		"defaultValue": "({value, text}) => {}",
		"version": "0.20.0",
		"require": "false"
	}
