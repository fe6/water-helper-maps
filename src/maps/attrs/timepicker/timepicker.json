		"defaultValue": "false",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/transfer": {
		"description": "是否转移到 body 下",
		"optionType": "Boolean",
		"defaultValue": "true",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/clearable": {
		"description": "是否显示清除按钮",
		"optionType": "Boolean",
		"defaultValue": "true",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/pickerStyle": {
		"description": "面板样式",
		"optionType": "String | Object",
		"defaultValue": "-",
		"version": "0.20.0",
		"require": "false"
