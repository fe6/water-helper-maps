{
	"w-timepicker/value": {
		"description": "绑定值",
		"optionType": "Boolean",
		"defaultValue": "moment().format('HH:mm:ss')",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/placeholder": {
		"description": "输入框占位文本",
		"optionType": "String",
		"defaultValue": "-",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/size": {
		"description": "设置大小",
    "options": [
      "small",
      "large",
      "不设置"
    ],
		"optionType": "String",
		"defaultValue": "-",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/format": {
		"description": "显示在输入框中的格式，参考 moment 的日期格式",
		"optionType": "String",
		"defaultValue": "HH:mm:ss",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/valueFormat": {
		"description": "绑定值的格式，参考 moment 的日期格式",
