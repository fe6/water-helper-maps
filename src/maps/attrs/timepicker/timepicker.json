		"defaultValue": "0",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/editable": {
		"description": "文本框可输入",
		"optionType": "Boolean",
		"defaultValue": "-",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/disabled": {
		"description": "设置禁用状态",
		"optionType": "Boolean",
		"defaultValue": "-",
		"version": "0.20.0",
		"require": "false"
	},
	"w-timepicker/open": {
		"description": "控制弹层是否展开",
