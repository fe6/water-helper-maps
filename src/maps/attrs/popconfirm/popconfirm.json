{
	"w-popconfirm/value": {
		"description": "显示状态",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-popconfirm/transfer": {
		"description": "是否转移到 body 下",
		"optionType": "Boolean",
		"defaultValue": "true"
	},
	"w-popconfirm/loading": {
		"description": "确定按钮的加载状态",
		"optionType": "Boolean",
		"defaultValue": "-"
	},
	"w-popconfirm/interval": {
		"description": "下拉框间距，单位 px",
		"optionType": "Number",
		"defaultValue": "4"
	},
	"w-popconfirm/okText": {
		"description": "设置确定文案",
		"optionType": "String",
		"defaultValue": "确定"
	},
	"w-popconfirm/cancelText": {
		"description": "设置取消文案",
		"optionType": "String",
		"defaultValue": "取消"
	},
	"w-popconfirm/placement": {
		"description": "气泡框位置",
    "options": [
      "top",
      "bottom",
      "topLeft",
      "topRight",
      "bottomLeft",
      "bottomRight",
      "left",
      "right",
      "leftTop",
      "leftBottom",
      "rightTop",
      "rightBottom"
    ],
		"optionType": "String",
		"defaultValue": "top"
	},
	"w-popconfirm/className": {
		"description": "组件最外层的 class 名字",
		"optionType": "String | Object | Array",
		"defaultValue": "-"
	},
	"w-popconfirm/getContainer": {
		"description": "浮层渲染父节点，默认渲染到 body 上。支持返回 Promise",
		"optionType": "Function",
		"defaultValue": "-"
	},
	"w-popconfirm/change": {
		"description": "改变触发，返回 状态 ( status )",
		"optionType": "Function",
		"defaultValue": "(status) => {}"
	},
	"w-popconfirm/ok": {
		"description": "点击确定触发，返回 状态 ( status )",
		"optionType": "Function",
		"defaultValue": "(status) => {}"
	},
	"w-popconfirm/cancel": {
		"description": "点击取消触发，返回 状态 ( status )",
		"optionType": "Function",
		"defaultValue": "(status) => {}"
	},
	"w-popconfirm/before": {
		"description": "确定之前触发的方法，返回 Promise 对象",
		"optionType": "Function",
		"defaultValue": "function() {return new Promise((resolve, reject) => {resolve();});}"
	}
}